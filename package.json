{
  "name": "attest",
  "version": "0.0.1",
  "description": "Contract management",
  "main": "index.js",
  "dependencies": {
    "apollo-errors": "^1.7.1",
    "apollo-server-express": "^1.1.7",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "chance": "^1.0.11",
    "classnames": "^2.2.5",
    "connect": "^3.6.5",
    "cookie": "^0.3.1",
    "cors": "^2.8.4",
    "css-loader": "^0.28.9",
    "deep-diff": "^1.0.0-pre.2",
    "express": "^4.15.4",
    "graphql": "^0.11.7",
    "graphql-tag": "^2.5.0",
    "graphql-tools": "^2.23.1",
    "http-proxy-middleware": "^0.17.4",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.5",
    "moment": "^2.21.0",
    "moment-range": "^3.0.3",
    "mongoose": "^4.12.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-module-resolver": "^3.1.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "standard": "^10.0.3"
  },
  "standard": {
    "globals": [
      "react"
    ]
  },
  "scripts": {
    "dev": "node index.js",
    "build": "next build",
    "start-next": "NODE_ENV=production forever start index.js",
    "start-graph": "NODE_ENV=production forever start ./graphql/compiled/server.js",
    "watch-graph": "babel ./graphql/input --watch --out-dir ./graphql/compiled",
    "watch-data": "babel ./data/input --watch --out-dir ./data/compiled",
    "watch-all": "npm run dev && npm run watch-graph",
    "compile-graph": "babel ./graphql/input --out-dir ./graphql/compiled"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/grabbeh/attest.git"
  },
  "babel": {
    "plugins": [
      [
        "transform-es2015-modules-commonjs",
        {
          "allowTopLevelThis": true
        }
      ]
    ]
  },
  "author": "",
  "bugs": {
    "url": "https://github.com/grabbeh/attest/issues"
  },
  "homepage": "https://github.com/grabbeh/attest#readme"
}
